-export([]).



%% ====================================================================
%% Internal functions
%% ====================================================================

Fun1([],T)->
   T;
Fun1([X1|L],T)->
   Fun1(L,[X1*2|T]).


t()->
   F1=fun()->Fun1() end,
   ptester:run(1, [
           {"haha",F1}
        ]
    ).