-module(demo3).

%% ====================================================================
%% API functions
%% ====================================================================
-export([main/0]).



%% ====================================================================
%% Internal functions
%% ====================================================================

randomList(0)->
	[];
randomList(N)->
  [random:uniform(100000)|randomList(N-1)].

randomData(0,M)->
	[];
randomData(N,M)->
	[randomList(M)|randomData(N-1,M)].

main()->
  Data=randomData(100, 1000).